<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.zhou.demo.demos.web.mapper.RegisterMapper">


    <insert id="insertDbDorGenId" parameterType="com.zhou.demo.demos.web.model.RegisterInfo"
            useGeneratedKeys="true" keyProperty="id" keyColumn="id">
        insert into t_register (name, phone, status)
        values (#{name}, #{phone}, #{status})
    </insert>
</mapper>